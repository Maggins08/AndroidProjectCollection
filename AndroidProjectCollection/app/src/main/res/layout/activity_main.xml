<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btnLayoutExercise"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:text="Layout Exercise"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />

    <Button
        android:id="@+id/btnButtonExercise"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:text="Button Exercise"
        app:layout_constraintBottom_toTopOf="@+id/btnCalculatorExercise"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnLayoutExercise"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btnCalculatorExercise"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:layout_marginTop="69dp"
        android:text="Calculator Exercise"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.033" />

    <Button
        android:id="@+id/btnMidterm"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:text="Midterm"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnCalculatorExercise"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btnConnect3"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:text="Connect Three Exercise"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnMidterm"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btnPassingIntents"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:text="Passing Intents"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnConnect3"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btnFragments"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:text="Fragments"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnPassingIntents"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btnMenus"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:text="Menus"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnFragments"
        app:layout_constraintVertical_bias="0.0" />
</androidx.constraintlayout.widget.ConstraintLayout>